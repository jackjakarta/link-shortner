{
  "name": "klikr-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 127.0.0.1",
    "dev:db": "chmod +x $(git rev-parse --show-toplevel)/dev/start-dev-db.sh && $(git rev-parse --show-toplevel)/dev/start-dev-db.sh",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "types": "tsc --noEmit",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "test": "jest --verbose",
    "getenv:dev": "chmod +x $(git rev-parse --show-toplevel)/scripts/fetch-env.sh && $(git rev-parse --show-toplevel)/scripts/fetch-env.sh local",
    "getenv:prod": "chmod +x $(git rev-parse --show-toplevel)/scripts/fetch-env.sh && $(git rev-parse --show-toplevel)/scripts/fetch-env.sh prod",
    "checks": "chmod +x $(git rev-parse --show-toplevel)/scripts/checks.sh && $(git rev-parse --show-toplevel)/scripts/checks.sh",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "dotenv -e .env tsx ./src/db/migrate.ts",
    "db:introspect": "dotenv -e .env -- drizzle-kit studio --config=src/db/drizzle.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.665.0",
    "@aws-sdk/s3-request-presigner": "3.758.0",
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-hover-card": "1.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-switch": "1.1.1",
    "@radix-ui/react-toast": "1.2.5",
    "@radix-ui/react-tooltip": "1.1.7",
    "@t3-oss/env-nextjs": "0.7.3",
    "@vercel/speed-insights": "1.1.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.33.0",
    "lodash": "4.17.21",
    "lucide-react": "0.439.0",
    "nanoid": "5.0.9",
    "next": "14.2.24",
    "next-auth": "4.24.5",
    "node-mailjet": "6.0.6",
    "openai": "4.72.0",
    "postgres": "3.4.3",
    "qrcode": "1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.52.2",
    "react-hot-toast": "2.4.1",
    "recharts": "2.12.7",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4",
    "zxcvbn": "4.4.2"
  },
  "packageManager": "pnpm@9.15.3",
  "devDependencies": {
    "@titanom/prettier-config": "2.0.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.14",
    "@types/node": "20.11.5",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/zxcvbn": "4.4.5",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "autoprefixer": "10.0.1",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "0.24.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "29.7.0",
    "postcss": "8.4.33",
    "tailwindcss": "3.3.0",
    "tailwindcss-motion": "1.0.1",
    "ts-jest": "29.2.5",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
